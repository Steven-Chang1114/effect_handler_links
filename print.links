fun printMessage() {
    do Print("Hello\n");
    do Print("World\n")
}

fun reverse(m) {
    handle(m()) {
        case Print(val, k) -> k(()); print(val)
        case Return(x) -> ()
    }
}

fun forward(m) {
    handle(m()) {
        case Print(val, k) -> print(val); k(())
        case Return(x) -> ()
    }
}


# reverse(printMessage)
forward(printMessage)